# a='all' l='long format'
alias ll='ls -al'
# A='almost all' do not list implied . and ..
alias la='ls -A'
alias ..='cd ..'
alias ...='cd ../..'

# git section
alias gs='git status'
alias ga='git add'
alias gl='git log'
alias brancha='git branch -a'

alias h='history'
alias c='clear'
alias n='nano'

alias B='nano ~/.bashrc'
alias R='source ~/.bashrc'
alias D='cd ~/Desktop'

alias startLAMP='/usr/local/bin/startLAMP.sh'
alias restartLAMP='/usr/local/bin/restartLAMP.sh'

alias npmi='npm install'
alias nvmls='nvm ls'