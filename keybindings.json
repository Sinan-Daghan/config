[
{
    "key": "ctrl+shift+\\",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
},


{
    "key" : "alt+right",
    "command" : "cursorEnd"
},
{
    "key" : "alt+left",
    "command" : "cursorHome"
},


{
    "key": "alt+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "alt+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
},


{
    "key": "ctrl+shift+enter",
    "command": "editor.action.insertLineBefore",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "ctrl+enter",
    "command": "editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly"
},


{
    "key": "shift+alt+up",
    "command": "editor.action.copyLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
},
{
    "key": "shift+alt+down",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
},


{
    "key": "ctrl+z",
    "command": "undo"
},
{
    "key": "ctrl+shift+z",
    "command": "redo"
},


{
    "key": "ctrl+.",
    "command": "type",
    "args": { "text": ">>" },
    "when": "editorTextFocus && resourceLangId == cpp"
},
{
    "key": "ctrl+,",
    "command": "type",
    "args": { "text": "<<" },
    "when": "editorTextFocus && resourceLangId == cpp"
},
{
    "key": "ctrl+;",
    "command": "type",
    "args": { "text": "::" },
    "when": "editorTextFocus && resourceLangId == cpp"
},


{
    "key": "oem_102",
    "command": "type",
    "args": { "text": "" }
},


{
    "key": "oem_102 ,",
    "command": "cursorLineStart",
    "args": { "text": "" }
},
{
    "key": "oem_102 .",
    "command": "cursorEnd",
    "args": { "text": "" }
},


{
    "key": "oem_102 ;",
    "command": "type",
    "args": { "text": "\n" },
    "when": "editorTextFocus"
},
{
    "key": "oem_102 space",
    "command": "type",
    "args": { "text": "\n" },
    "when": "editorTextFocus"
},


{
    "key": "oem_102 d",
    "command": "type",
    "args": { "text": "{" },
    "when": "editorTextFocus"
},
{
    "key": "oem_102 f",
    "command": "type",
    "args": { "text": "(" },
    "when": "editorTextFocus"
},


{
    "key": "oem_102 j",
    "command": "type",
    "args": { "text": "(" },
    "when": "editorTextFocus"
},
{
    "key": "oem_102 k",
    "command": "type",
    "args": { "text": "{" },
    "when": "editorTextFocus"
}
]